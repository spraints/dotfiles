ConnectTimeout 16
<% if ssh.usekeychain %>
UseKeychain yes
<% end %>
# AddKeysToAgent yes # <-- not sure about this

Host 68k.org
  User spraints

Host laptop
  User spraints
  HostName 10.0.2.2

Host minibuntu.local, minibuntu
  HostName minibuntu.local
  User matt

Host <%= ssh.githubhostwildcard %>
  User <%= ssh.githubuser %>

Host <%= ssh.bastion %>
  ForwardAgent yes
  User <%= ssh.githubuser %>
<% ssh.githubhosts.each do |short, full| %>
Host <%= short %>
  HostName <%= full %>
  ForwardAgent yes
  User <%= ssh.githubuser %>
<% end %>
